upstream django {
    # server unix:///path/to/your/mysite/mysite.sock; # for a file socket
    server 172.19.6.53:8000; # for a web port socket (we'll use this first)
}

# configuration of the server
server {
    # the port your site will be served on
    listen      80;
    # the domain name it will serve for
    server_name www.mutougeda.cn; # substitute your machine's IP address or FQDN
    charset     utf-8;

    # max upload size
    client_max_body_size 75M;   # adjust to taste

    # Django media
    location /media  {
        alias /var/www/web_MTGD/media;  # your Django project's media files - amend as required
    }

    location /static {
        alias /var/www/web_MTGD/static; # your Django project's static files - amend as required
    }

    # Finally, send all non-media requests to the Django server.
    location / {
        uwsgi_pass  django;
        include     /home/web_sites/mutougeda_cn/MTGD_syn_01/uwsgi_params; # the uwsgi_params file you installed
    }
}
